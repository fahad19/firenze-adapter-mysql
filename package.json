{
  "name": "firenze-adapter-mysql",
  "version": "0.1.4",
  "description": "MySQL adapter for firenze.js ORM",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "babel:watch": "babel --watch src --out-dir lib",
    "lint": "eslint src",
    "mocha": "mocha $(find lib/test/cases -name '*.js')",
    "test": "run-scripts lint babel mocha",
    "istanbul": "istanbul cover ./node_modules/mocha/bin/_mocha $(find lib/test/cases -name '*.js')",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "run-scripts istanbul coveralls",
    "doctoc": "doctoc README.md --github --title '# Contents'",
    "docume": "docume README.md",
    "docs": "npm run docume && npm run doctoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fahad19/firenze-adapter-mysql.git"
  },
  "keywords": [
    "firenze",
    "firenze-adapter",
    "firenze-plugin",
    "orm",
    "mysql",
    "database",
    "adapter"
  ],
  "author": "Fahad Ibnay Heylaal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fahad19/firenze-adapter-mysql/issues"
  },
  "homepage": "https://github.com/fahad19/firenze-adapter-mysql",
  "dependencies": {
    "knex": "~0.8.3",
    "lodash": "~3.7.0",
    "mysql": "~2.6.2"
  },
  "peerDependencies": {
    "firenze": "~0.1.4"
  },
  "devDependencies": {
    "babel": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "coveralls": "^2.11.2",
    "doctoc": "^0.13.0",
    "docume": "^0.1.4",
    "eslint": "^0.20.0",
    "firenze": "~0.1.4",
    "istanbul": "^0.3.16",
    "mocha": "^2.2.4",
    "run-scripts": "^0.2.2",
    "should": "^6.0.1",
    "should-promised": "^0.2.1"
  }
}
